<script type="text/javascript"> 
function displaySubMenu(li) { 
var subMenu = li.getElementsByTagName("div")[0]; 
subMenu.style.display = "block"; 
} 
function hideSubMenu(li) { 
var subMenu = li.getElementsByTagName("div")[0]; 
subMenu.style.display = "none"; 
} 

function trans(){
    var uid = document.form1.uid.value; 
    var pass = document.form1.pass.value; 
	if(uid == ""){
            alert("用户名不能为空");
            return false;
	}

        if (uid.substring(0,4) == 'fang'){
	    document.form1.action="http://mis.njtu.edu.cn/htmls/bg/gg/ggzldl.jsp";
	    document.form1.submit();
            return true;
	}

        if(uid.length<=7 || uid.substring(0,2) == "98" || uid.substring(0,2) == "97" ||uid.substring(2,5)=="984" ){
		document.form1.password.value=pass;
		document.form1.loginname.value=uid;
		document.form1.action="http://pymis.njtu.edu.cn/auth/login/";
		document.form1.submit();
                return true;
        }

        if(uid.length>7 && uid.substring(2,3) == "1"  ){
		document.form1.password.value=pass;
		document.form1.loginname.value=uid;
		document.form1.action="http://pymis.njtu.edu.cn/auth/login/";
	        document.form1.submit();
                return true;
	}else{
	        document.form1.action="http://smis.bjtu.edu.cn/std_Verify.jsp";
                document.form1.submit();
                return true;
        }
}       

</script>

